@model List<MeuProjeto.Models.UsuarioModel>

<!DOCTYPE html>
<html>
<head>
    <title>Tela de Usuários</title>
</head>
<body>
    <h2>Lista de Usuários</h2>
    <table>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Email</th>
            <th></th>
        </tr>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.Id</td>
                <td>@usuario.Nome</td>
                <td>@usuario.Email</td>
                <td><a href="@Url.Action("Edit", new { id = usuario.Id })">Editar</a></td>
            </tr>
        }
    </table>
</body>
</html>
